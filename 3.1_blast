#mites
nohup blastn -perc_identity 60 -task blastn -query data/results/mites_clustered.fasta -subject  data/fasta/potato_dm_v404_all_pm_un.fasta  -outfmt '6 qseqid sseqid qstart qend sstart send score length mismatch gaps gapopen nident pident qlen slen qcovs' -evalue 10e-3 -qcov_hsp_perc 80 > data/results/mites_clustered.blast.csv  &

